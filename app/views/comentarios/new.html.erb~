<h1> Novo Comentario </h1>

<% form_for (@comentario) do |f| %>
  <%= f.error_messages %>

  <%= f.hidden_field :comentavel_id %>
  <%= f.hidden_field :comentavel_type %>

  <p> 
    <%= f.label :conteudo %> <br/>
    <%= f.text_area :conteudo %>
  </p>

  <p>
    <%= f.submit "criar" %>
  </p>
<% end %>



