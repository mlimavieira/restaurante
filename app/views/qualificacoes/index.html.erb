<h1>Listagem de qualificacoes</h1>

<table border="1">
  <tr>
    <th>Cliente</th>
    <th>Restaurante</th>
    <th>Nota</th>
    <th>Valor gasto</th>
    <th>...</th>
  </tr>

<% @qualificacoes.each do |qualificacao| %>
  <tr>
    <td><%=h qualificacao.cliente.nome %></td>
    <td><%=h qualificacao.restaurante.nome %></td>
    <td><%=h qualificacao.nota %></td>
    <td><%=h qualificacao.valor_gasto %></td>
    <td><%= link_to 'Exibir', qualificacao %>&nbsp;
	<%= link_to 'Alterar', edit_qualificacao_path(qualificacao) %>&nbsp;
	<%= link_to 'Deletar', qualificacao, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nova qualificacao', new_qualificacao_path %>
